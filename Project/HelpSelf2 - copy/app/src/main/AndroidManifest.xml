<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.helpself2">

    <uses-permission android:name="android.permission.INTERNET" />
    //存取網路的權限
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    //存取GPS定位權限
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    //網路定位權限
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    //裝置不進入睡眠權限
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SmsActivity" />
        <activity android:name=".SosActivity" />
        <activity android:name=".RBlightActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".HPActivity" />
        <activity android:name=".PlayActivity" />
        <activity android:name=".SafeActivity" />
        <activity android:name=".CompassActivity" />
        <activity android:name=".WildMountain" />
        <activity android:name=".WildSnake" />
        <activity android:name=".EatFungus" />
        <activity android:name=".EatVegetables" />
        <activity android:name=".CprVideoActivity" />
        <activity android:name=".CprPdf" />
        <activity android:name=".SettingActivity"></activity>
    </application>

</manifest>